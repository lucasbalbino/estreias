{
  "name": "estreias",
  "version": "1.0.0",
  "description": "Todas as estreias de Cinema, Netflix e HBO Go",
  "main": "index.js",
  "scripts": {
    "client-dev": "cd client && npm run start",
    "server-dev": "nodemon server.js",
    "client": "cd client && npm run start",
    "server": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server-dev\" \"npm run client-dev\"",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.2",
    "mysql": "^2.15.0",
    "react-helmet": "^5.2.0",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasbalbino/estreias.git"
  },
  "author": "Lucas Balbino",
